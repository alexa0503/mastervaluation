<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<meta name="format-detection" content="telephone=no" />
<meta name="apple-mobile-web-app-capable" content="yes" />
<title>{% block title %}物业测算{% endblock %}</title>

<link rel="stylesheet" href="{{ asset('bundles/app/default/') }}css/common.css">
<link rel="stylesheet" href="{{ asset('bundles/app/default/') }}css/select2.css">
<script src="{{ asset('bundles/app/default/') }}js/jquery-1.9.1.min.js"></script>
<script src="{{ asset('bundles/app/default/') }}js/select2.min.js"></script>
<script src="{{ asset('bundles/app/default/') }}js/common.js"></script>
<!--移动端版本兼容 -->
<script type="text/javascript">
         var phoneWidth =  parseInt(window.screen.width);
         var phoneScale = phoneWidth/640;
         var ua = navigator.userAgent;
         if (/Android (\d+\.\d+)/.test(ua)){
                   var version = parseFloat(RegExp.$1);
                   if(version>2.3){
                            document.write('<meta name="viewport" content="width=640, minimum-scale = '+phoneScale+', maximum-scale = '+phoneScale+', target-densitydpi=device-dpi">');
                   }else{
                            document.write('<meta name="viewport" content="width=640, target-densitydpi=device-dpi">');
                   }
         } else {
                   document.write('<meta name="viewport" content="width=640, user-scalable=no, target-densitydpi=device-dpi">');
         }
</script>
<!--移动端版本兼容 end -->
</head>
<body>
	{% block body %}{% endblock %}
	{% block javascripts %}
	{% endblock %}
	

</body>
</html>
